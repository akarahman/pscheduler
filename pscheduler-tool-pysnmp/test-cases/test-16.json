{
	"_comment": "This is a test for an SNMPGET VERSION 3 query with SHA AUTHENTICATION and AES encryption.",
	"test": {
		"type": "snmpget",
		"spec": {
			"schema": 1,
			"dest": "demo.snmplabs.com",
			"_community": "public",
			"version": "3",
			"security-name": "usr-sha-aes",
			"security-level": "authPriv",
			"auth-protocol": "sha",
			"priv-protocol": "aes",
			"_auth-key": "authkey1",
			"_priv-key": "privkey1",
			"oid": ["IF-MIB::ifInOctets.1"],
			"polls": 1
		}
	}
}